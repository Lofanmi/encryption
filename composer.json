{
    "name": "larryphp/encryption",
    "description": "A tiny encryptor from Laravel.",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "LarryPHP\\Encryption\\": "src/"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "paragonie/random_compat": "~1.4|~2.0",
        "indigophp/hash-compat": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "minimum-stability": "dev"
}
